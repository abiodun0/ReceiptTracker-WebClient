    "@webpack-blocks/css-modules": "^0.3.0",
    "@webpack-blocks/dev-server2": "^0.3.0",
    "@webpack-blocks/extract-text2": "^0.3.0",
    "@webpack-blocks/postcss": "^0.3.0",
    "@webpack-blocks/sass": "^0.3.0",
    "@webpack-blocks/webpack2": "^0.3.1",
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-relative-import": "^1.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
