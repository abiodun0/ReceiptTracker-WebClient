{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Desktop revamp webpack setup for SCSS and accompanying components",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild:webpack": "rm -rf static/dist && rm -rf build",
    "build:webpack": "NODE_ENV=production webpack --config ./webpack/webpack.config.js --display-error-details",
    "prebuild:server": "rm -rf dist",
    "build:server": "babel ./src -d ./dist -D",
    "start": "NODE_ENV=development PORT=9006 webpack-dev-server --config ./webpack/webpack.config.js & NODE_ENV=development PORT=9007 node ./rootServer.js",
    "pre-lint": "(cd \"./node_modules/\" && pwd)",
    "preinstall": "npm prune",
    "postinstall": "rsync pre-commit .git/hooks && chmod 755 .git/hooks/pre-commit",
    "analyze:build": "NODE_ENV=production webpack --json --config ./webpack/webpack.config.js  > bundle-stats.json",
    "analyze:json": "webpack-bundle-size-analyzer bundle-stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abiodun0/webpack2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abiodun0/webpack2/issues"
  },
  "homepage": "https://github.com/abiodun0/webpack2#readme",
